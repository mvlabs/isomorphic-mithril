  "engines": {
    "node": ">= 10.0.0",
    "npm": ">=6.0.0"
  },
  "description": "Isomorphic JS with Mithril",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run watchall",
    "watch": "webpack --mode=development --watch --config webpack.config.js",
    "watchall": "concurrently \"nodemon --config nodemon.json\" \"npm run watch\"",
    "build": "webpack --mode=production --config webpack.config.js",
    "production": "node index.js",
    "clean": "rimraf dist/*"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "body-parser": "^1.18.3",
    "browser-sync": "^2.26.3",
    "bulma": "^0.7.2",
    "cookie-parser": "^1.4.3",
    "decode-html": "^2.0.0",
    "esm": "^3.0.84",
    "express": "^4.16.4",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "marked": "^0.5.1",
    "mithril": "^2.0.0-rc.0",
    "mithril-node-render": "^2.3.1",
    "nodemon": "^1.18.5",
    "prismjs": "^1.15.0",
    "rimraf": "^2.6.2",
    "store": "^2.0.12",
    "string-hash": "^1.1.3",
